{% for group, argu in salt['pillar.get']('logins', {}).items() %}
{% for host, args in argu.items() %}
define host{
        use                     generic-host
        contact_groups          admins
        host_name               {{ host }}
        address                 {{ args.ip }}
        hostgroups              login_{{group}}
        }
{% endfor %}{% endfor %}



